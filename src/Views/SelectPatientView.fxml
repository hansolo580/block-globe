<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" fx:controller="Controllers.SelectPatient">
    <children>
        <ListView fx:id="listViewPatient" layoutX="200.0" layoutY="48.0" prefHeight="302.0" prefWidth="370.0"
                  AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
                  AnchorPane.topAnchor="48.0"/>
        <Button fx:id="buttonQuit" layoutX="333.0" layoutY="361.0" mnemonicParsing="false" onAction="#quit" text="Back"
                AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="buttonSelect" layoutX="277.0" layoutY="355.0" mnemonicParsing="false" onAction="#select"
                text="Select" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="65.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Label layoutX="128.0" layoutY="14.0" text="Select Patient">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
